<%- include('layouts/header') %>


<!-- Tranding news  carousel-->
<section class="bg-light">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="wrapp__list__article-responsive wrapp__list__article-responsive-carousel">
                    <% header_news.forEach(i => { %>
                        <div class="item">
                            <!-- Post Article -->
                            <div class="card__post card__post-list">
                                <div class="image-sm">
                                    <a href="/article-detail/<%= i.id %>">
                                        <img src="/uploads/<%= i.avatar[0] %>" class="img-fluid" alt="">
                                    </a>
                                </div>
                                <div class="card__post__body ">
                                    <div class="card__post__content">
                                        <div class="card__post__author-info mb-2">
                                            <ul class="list-inline">
                                                <li class="list-inline-item">
													<span class="text-primary">
														<%= i.author %>
													</span>
                                                </li>
                                                <li class="list-inline-item">
													<span class="text-dark text-capitalize date">
														<%= i.created_at %>
													</span>
                                                </li>

                                            </ul>
                                        </div>
                                        <div class="card__post__title">
                                            <h6>
                                                <a href="/article-detail/<%= i.id %>">
                                                    <%= i.title %>
                                                </a>
                                            </h6>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    <% }) %>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Tranding news carousel -->


